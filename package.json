{
  "name": "heroku-starter-expressjs",
  "version": "0.1.0",
  "description": "A dockerized starter project with expressjs backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "start:dev": "nodemon -L ./src/index.js",
    "test": "jest",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint ./src/**/*.js --fix",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ghost---Shadow/heroku-starter-expressjs.git"
  },
  "keywords": [
    "expressjs",
    "heroku"
  ],
  "author": "Souradeep Nanda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ghost---Shadow/heroku-starter-expressjs/issues"
  },
  "homepage": "https://github.com/Ghost---Shadow/heroku-starter-expressjs#readme",
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0"
  }
}
